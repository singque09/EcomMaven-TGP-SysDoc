<table cellpadding="0" cellspacing="0" border="0" width="350px" align="center">
	<tr>
		<td><table cellpadding="0" cellspacing="0" border="0" width="100%">
			<tr>
				<td >&nbsp;</td>
				<td  valign="middle" rowspan="3" align="right">
					<b><font style="font-size:24px;text-transform:uppercase;">[%NOHTML%][@ship_port@][%END NOHTML%]</font></b>
				</td>
				<td >&nbsp;</td>
			</tr>
			<tr>
				<td><font style="font-size:14px;text-transform:uppercase;"><b>Couriers Please</b></font></td>
				<td>Service [@shserv_code@]</td>
				<td>&nbsp;</td>
			</tr>
		</table></td>
	</tr>
	<tr>
		<td>&nbsp;</td>
	</tr>
	<tr>
		<td><table cellpadding="0" cellspacing="0" border="0" width="100%">
			<td >Connote:<font style="font-size:18px;padding-left:10px;"><b>[%NOHTML%][@cngmt_number@][%END NOHTML%]</b></font></td>
			[%IF [@signature@] eq 'n'%]
			<td style="border-style:solid;"><font style="font-size:18px;padding-left:10px"><b>ATL </b></font></td>
			[%ELSE%]
			<td>&nbsp;</td>
			[%/IF%]
		</table></td>
	</tr>
	<tr>
		<td>
			<table cellpadding="0" cellspacing="0" border="0" width="100%">
				<tr><td valign="top">To:</td>
					<td>
						<font style="font-size:18px;text-transform:uppercase;" >
							[%NOHTML%][@ship_first_name@] [@ship_last_name@][%END NOHTML%]<br />
							[%DATA id:'ship_company' if:'ne' value:''%][%NOHTML%][@ship_company@][%END NOHTML%]<br>[%END DATA%]
							[%NOHTML%][@ship_street1@][%END NOHTML%]<br>
							[%DATA id:'ship_street2' if:'ne' value:''%][%NOHTML%][@ship_street2@][%END NOHTML%]<br>[%END DATA%]
						</font>
						<table cellpadding="0" cellspacing="0" border="0" width="100%" align="center">
							<tr><td width="70%" valign="top">
								<font  style="font-size:18px; text-transform:uppercase;">[%NOHTML%][@ship_city@][%END NOHTML%]</font></td>
								<td width="30%" align="right" valign="top">
									<font  style="font-size:13px; text-transform:uppercase;">[%NOHTML%][@ship_state@][%END NOHTML%] </font>&nbsp;
									<font  style="font-size:18px; text-transform:uppercase;"> [%NOHTML%][@ship_zip@][%END NOHTML%]</font>
								</td></tr>
						</table>
					</td></tr>
			</table>
		</td></tr>
	<tr><td>&nbsp;</td></tr>
	<tr><td style="padding-bottom:8px;">
		<table cellpadding="0" cellspacing="0" border="0" width="100%" align="center">
			<tr>
				<td width="20%" align="left">
					<font  style="font-size:12px;"> Items [@DATA:TOTAL@]</font>
				</td>
				<td width="40%" align="right">
					<font  style="font-size:12px;">Weight [%NOHTML%][%FORMAT type:'number' dp:'3'%][@total_weight@][%END FORMAT%][%END NOHTML%] kg</font>
				</td>
				<td width="40%" align="right">
					<font  style="font-size:12px;">Cube [%NOHTML%][%FORMAT type:'number' dp:'3'%][@total_cubic@][%END FORMAT%][%END NOHTML%] m3</font>
				</td>
			</tr>
			<tr><td>&nbsp;</td></tr>
			<tr>
				<td colspan="3"><b>Contact:</b>&nbsp;<font style="font-size:12px;">[%NOHTML%][@ship_first_name@][%END NOHTML%]&nbsp;[%NOHTML%][@ship_last_name@][%END NOHTML%][%DATA id:'ship_company' if:'ne' value:''%]<br/>[%NOHTML%][@ship_company@][%END NOHTML%][%END DATA%]</font></td>
			</tr>
			<tr>
				<td colspan="3"><b>Phone:</b>&nbsp;<font style="font-size:12px; text-transform:uppercase;">[%NOHTML%][@ship_phone@][%END NOHTML%]</font></td>
			</tr>
			<tr>
				<td colspan="3"><b>Reference:</b>&nbsp;<font style="font-size:12px; text-transform:uppercase;">[%NOHTML%][@order_id@][%END NOHTML%]</font></td>
			</tr>

		</table>
	</td>
	</tr>

	<tr><td style="padding-bottom:8px;">
		<table cellpadding="0" cellspacing="0" border="0" width="100%" align="center">
			<tr>
				<td valign="top">
					<font  style="font-size:12px;">From: </font>
				</td>
				<td valign="top" >
					<font  style="font-size:12px;">[%DATA id:'bill_company' if:'ne' value:''%][%NOHTML%][@bill_company@][%END NOHTML%]<br>[%END DATA%][%NOHTML%][@bill_street1@][%END NOHTML%]<br>
						[%DATA id:'bill_street2' if:'ne' value:''%][%NOHTML%][@bill_street2@][%END NOHTML%]<br>[%END DATA%]<br />
						[%NOHTML%][@bill_city@][%END NOHTML%]<br /> </font>
				</td>
				<td align="right" valign="top">
					<font  style="font-size:12px;">Date: </font>[%FORMAT type:'date' now:1 format:'#d/ #M/ #Y' default:'n/a'%][@label_created@][%END FORMAT%]<br/>&nbsp;<br/>&nbsp;<br/>[%NOHTML%][@bill_zip@][%END NOHTML%] &nbsp;
					<font  style="font-size:12px; text-transform:uppercase;">
						[%NOHTML%][@bill_state@][%END NOHTML%]</font>
				</td>
			</tr>
		</table>
	</td>
	</tr>
	<tr><td>&nbsp;</td></tr>
	[%IF [@danger@] eq 'n'%]
	<tr>
		<td valign="top">
			<font style="font-size:9px;">
				<b>Aviation Security and Dangerous Goods Declaration</b>
				<br />
				The sender acknowledges that this article may be carried by air and will be subject to
				aviation security and clearing procedures; and the
				sender declares that the article does not contain any dangerous or prohibited goods, explosive or
				incendiary devices. A false declaration is a criminal offence</font><br />
		</td>
	</tr>
	<tr><td>&nbsp;</td></tr>
	[%/IF%]
	<tr><td style="padding-bottom:8px;">
		<table cellpadding="0" cellspacing="0" border="0" width="100%" align="center">
			<tr><td align="center" style="padding-bottom:8px;">
				<img width="290px" src="[%BARCODE_URL fn:'article' height:'40' cngmt:'[@cngmt_id@]' art:'[@art_id@]'%][@barcode@][%END BARCODE_URL%]" border="0"/>
			</td>
			</tr>
			<tr><td align="center">
				<b><font style="font-size:12px;">[%NOHTML%][@article_number@][%END NOHTML%]</font></b>
			</td></tr>
			<tr><td>&nbsp;</td></tr>
			<tr><td>&nbsp;</td></tr>
			<tr><td align="center">
				<font  style="font-size:18px;">
					<b>ITEM:
						[@DATA:COUNT@] OF [@DATA:TOTAL@]</b></font>
			</td></tr>
		</table>
	</td>
	</tr>
	<tr>
		<td style="padding-bottom:8px;">
			<b>Delivery Instructions:</b><br>
			<font style="font-size:10px; text-transform:uppercase;">
				[%DATA id:'ship_comment' if:'ne' value:''%][%PARAM if_true%]
				[%NOHTML%][%FORMAT type:'text' maxlength:'255'%][@ship_comment@][%END FORMAT%][%END NOHTML%]
				[%END PARAM%][%PARAM if_false%]
				n/a
				[%END PARAM%][%END DATA%]
			</font>
		</td>
	</tr>
	<tr>
		<td align="center"><font style="font-size:8px;">Printed With Neto E-commerce Software www.neto.com.au</font></td>
	</tr>
</table>
